  </servlet>
  <servlet-mapping>
  	<servlet-name>savenote</servlet-name>
  	<url-pattern>/savenote</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DeleteServlet</servlet-name>
  	<url-pattern>/DeleteServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
